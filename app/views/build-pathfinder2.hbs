        <a id='iconic-select-generic' data-set-id='iconic-image-list-generic' class='selected'>
          <span class='group'>{{__ "Dyslexic Studeos"}}</span>
          <span class='name'>{{__ "General purpose"}}</span>
        </a>
        {{#each iconicGroups}}
