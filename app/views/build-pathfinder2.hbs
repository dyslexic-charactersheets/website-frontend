                <ul class='wide'>
                  {{#each items}}
                    <li><label for='class-{{e code}}'><input type='radio' id='class-{{e code}}' name='class' value='{{id}}' data-reveal="{{e code}}"> {{__ name}}</label></li>
                  {{/each}}
                </ul>
              </div>
              {{/each}}
              
              <div class='select-group-more'>
                <h4>{{__ "Other"}}</h4>
                <div class="yui3-g">
                  <div class="yui3-u-1-3">
                    <ul>
                      <li><label for='class-generic'><input type='radio' id='class-generic' name='class' value='generic' checked> {{__ "Other"}}</label></li>
                    </ul>
                  </div>
                  <div class="yui3-u-2-3">
                    {{#each classGroups}}{{#unless core}}
                    <a class='btn reveal-group' data-reveal="select-group-class-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                    {{/unless}}{{/each}}
                  </div>
                </div>
              </div>
            </fieldset>

          </div>
          <div class="yui3-u-1-2">
            {{!-- Subclasses --}}
            {{#each classes}}
            <div class='reveal reveal-subclass' id='reveal-subclass-{{e code}}'>
              {{#each selects}}
                <h3>{{__ name}}</h3>

                <fieldset>
                  {{#each valueGroups}}
                  <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-{{e ../../code}}-{{slug ../name}}-{{id}}'>
                    <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">&ndash;</a>{{/unless}}</h4>
                    <ul class="wide">
                      {{#each items}}
                        {{#if ../../multiselect}}
                          <li><label for='{{e ../../select}}-{{e code}}'><input type='checkbox' id='{{e ../../select}}-{{e code}}' data-class-option='{{e ../../select}}' name='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{else}}
                          <li><label for='subclass-{{e ../../select}}-{{e code}}'><input type='radio' id='subclass-{{e ../../select}}-{{e code}}' name='{{e ../../select}}' value='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{/if}}
                      {{/each}}
                    </ul>
                  </div>
                  {{/each}}

                  <div class="select-group-more">
                    {{#unless multiselect}}
                    <ul>
                      <li><label for='subclass-{{e select}}-none'><input type='radio' id='subclass-{{e select}}-none' name='{{e select}}' value='none' checked> {{__ "None"}}</label></li>
                    </ul>
                    {{/unless}}
                    {{#each valueGroups}}{{#unless core}}
                    <a class='btn reveal-group' data-reveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                    {{/unless}}{{/each}}
                  </div>
                </fieldset>
              {{/each}}
            </div>
            {{/each}}

          </div>
        </div>
        

        <hr>


        {{!-- Multiclass --}}
        <h2>{{__ "Multiclass"}}</h2>
        <div class="yui3-g">
          <div class="yui3-u-1-2">

            <fieldset id='multiclass'>
              {{#each multiclassGroups}}
              <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-multiclass-{{id}}'>
                <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal='select-group-multiclass-{{id}}'>&ndash;</a>{{/unless}}</h4>
                <ul class='wide'>
                  {{#each items}}
                    <li><label for='archetype-{{e code}}'><input type='checkbox' id='archetype-{{e code}}' class='archetype' name='{{id}}' data-reveal="{{e code}}"> {{__ name}}</label></li>
                  {{/each}}
                </ul>
              </div>
              {{/each}}
              
              <div class='select-group-more'>
                <h4>{{__ "Other"}}</h4>
                {{#each multiclassGroups}}{{#unless core}}
                <a class='btn reveal-group' data-reveal="select-group-multiclass-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                {{/unless}}{{/each}}
              </div>
            </fieldset>

          </div>
          <div class="yui3-u-1-2">

            {{!-- Sub-archetypes --}}
            {{#each multiclass}}
            <div class='reveal reveal-subarchetype' id='reveal-subarchetype-{{e code}}'>
              {{#each selects}}
                <h3>{{__ name}}</h3>

                <fieldset>
                  {{#each valueGroups}}
                  <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-subarch-{{e ../../code}}-{{slug ../name}}-{{id}}'>
                    <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">&ndash;</a>{{/unless}}</h4>
                    <ul class="wide">
                      {{#each items}}
                        {{#if ../../multiselect}}
                          <li><label for='subarch-{{e ../../select}}-{{e code}}'><input type='checkbox' id='subarch-{{e ../../select}}-{{e code}}' data-class-option='{{e ../../select}}' name='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{else}}
                          <li><label for='subarch-{{e ../../select}}-{{e code}}'><input type='radio' id='subarch-{{e ../../select}}-{{e code}}' name='{{e ../../select}}' value='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{/if}}
                      {{/each}}
                    </ul>
                  </div>
                  {{/each}}

                  <div class="select-group-more">
                    {{#unless multiselect}}
                    <ul>
                      <li><label for='subarch-{{e select}}-none'><input type='radio' id='subarch-{{e select}}-none' name='{{e select}}' value='none' checked> {{__ "None"}}</label></li>
                    </ul>
                    {{/unless}}
                    {{#each valueGroups}}{{#unless core}}
                    <a class='btn reveal-group' data-reveal="select-group-subarch-{{e ../../code}}-{{slug ../name}}-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                    {{/unless}}{{/each}}
                  </div>
                </fieldset>
              {{/each}}
            </div>
            {{/each}}
          </div>
        </div>


        <hr>


        {{!-- Archetypes --}}
        <h2>{{__ "Archetype"}}</h2>
        
        <div class="yui3-g">
          <div class="yui3-u-1-2">

            <fieldset id='classes'>
              {{#each archetypeGroups}}
              <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-archetype-{{id}}'>
                <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal='select-group-archetype-{{id}}'>&ndash;</a>{{/unless}}</h4>
                <ul class='wide'>
                  {{#each items}}
                    <li><label for='archetype-{{e code}}'><input type='checkbox' id='archetype-{{e code}}' class='archetype' name='{{id}}' data-reveal="{{e code}}"> {{__ name}}</label></li>
                  {{/each}}
                </ul>
              </div>
              {{/each}}
              
              <div class='select-group-more'>
                <h4>{{__ "Other"}}</h4>
                {{#each archetypeGroups}}{{#unless core}}
                <a class='btn reveal-group' data-reveal="select-group-archetype-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                {{/unless}}{{/each}}
              </div>
            </fieldset>

          </div>
          <div class="yui3-u-1-2">

            {{!-- Sub-archetypes --}}
            {{#each archetypes}}
            <div class='reveal reveal-subarchetype' id='reveal-subarchetype-{{e code}}'>
              {{#each selects}}
                <h3>{{__ name}}</h3>

                <fieldset>
                  {{#each valueGroups}}
                  <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-subarch-{{e ../../code}}-{{slug ../name}}-{{id}}'>
                    <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">&ndash;</a>{{/unless}}</h4>
                    <ul class="wide">
                      {{#each items}}
                        {{#if ../../multiselect}}
                          <li><label for='subarch-{{e ../../select}}-{{e code}}'><input type='checkbox' id='subarch-{{e ../../select}}-{{e code}}' data-class-option='{{e ../../select}}' name='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{else}}
                          <li><label for='subarch-{{e ../../select}}-{{e code}}'><input type='radio' id='subarch-{{e ../../select}}-{{e code}}' name='{{e ../../select}}' value='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{/if}}
                      {{/each}}
                    </ul>
                  </div>
                  {{/each}}

                  <div class="select-group-more">
                    {{#unless multiselect}}
                    <ul>
                      <li><label for='subarch-{{e select}}-none'><input type='radio' id='subarch-{{e select}}-none' name='{{e select}}' value='none' checked> {{__ "None"}}</label></li>
                    </ul>
