<form id='build-my-character'>

    <header id='bodyheader'>
        <h1>{{__ "Build a character"}}: {{__ "Pathfinder 2nd Edition"}}</h1>
        
        <nav class='tabs'>
            <ul>
                <li><a href='/'>&laquo; {{__ "Back to intro"}}</a></li>
                <li><a rel='#start-tab' class='selected' id='start-tab-link'>{{__ "Start"}}</a></li>
                <li><a rel='#class-tab' class='start-hidden' id='class-tab-link'>{{__ "Class"}}</a></li>
                <li><a rel='#options-tab' class='start-hidden' id='options-tab-link'>{{__ "Options"}}</a></li>
                <li><a rel='#gm-start-tab' class='start-hidden' id='gm-start-tab-link'>{{__ "Game Master"}}</a></li>
                <li><a rel='#gm-options-tab' class='start-hidden' id='gm-options-tab-link'>{{__ "Options"}}</a></li>
                <li><a rel='#appearance-tab' class='start-hidden' id='appearance-tab-link'>{{__ "Appearance"}}</a></li>
                <li><a rel='#download-tab' class='start-hidden' id='download-tab-link'>{{__ "Download"}}</a></li>
            </ul>
        </nav>
    </header>
    

    <section class='tab selected' id='start-tab'>
      <h2 id='start-tab-title'>{{gameData.name}}</h2>
      
      <div id='start-options'>
        <a id='start-single'>
          <img src='/images/single.png'/>
          {{__ "One character"}}
        </a>
        {{!-- <a id='start-party'>
          <img src='/images/{{#if gameData.isStarfinder}}starfinder-party.png{{else}}party.png{{/if}}'/>
          {{__ "A whole party"}}
        </a> --}}
        <a id='start-gm'>
          <img src='/images/gm.png'/>
          {{__ "Dungeon Master"}}
        </a>
      </div>

      <input type='hidden' id='start-type' name='start-type'/>
    </section>


    <section class='tab' id='class-tab'>
        <p><a class='button' id='quick-link-build'>{{__ "Build a character"}}</a> &nbsp; {{__ "Download the single page beginner's guide."}}</p>
        <hr>

        <h2>{{__ "Name"}}</h2>
        <fieldset>
          <input id='character-name' class='name'>
          {{!-- <h3>{{__ "Description"}}</h3>
          <textarea rows='4' id='character-description'></textarea> --}}
        </fieldset>


        <hr>


        {{!-- Ancestries --}}
        <h2>{{__ "Ancestry"}}</h2>
        <div class="yui3-g">
          <div class="yui3-u-1-2">

            <fieldset id='ancestries'>

              {{#each ancestryGroups}}
              <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-{{id}}'>
                <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal="select-group-{{id}}">&ndash;</a>{{/unless}}</h4>
                <ul class='wide'>
                  {{#each items}}
                    <li><label for='ancestry-{{e code}}'><input type='radio' id='ancestry-{{e code}}' name='ancestry' value='{{id}}' data-reveal="{{e code}}"> {{__ name}}</label></li>
                  {{/each}}
                </ul>
              </div>
              {{/each}}
              
              <div class='select-group-more'>
                <h4>{{__ "Other"}}</h4>
                <div class="yui3-g">
                  <div class="yui3-u-1-3">
                    <ul>
                      <li><label for='ancestry-other'><input type='radio' id='ancestry-other' name='ancestry' value='other' checked> {{__ "Other"}}</label></li>
                    </ul>
                  </div>
                  <div class="yui3-u-2-3">
                    {{#each ancestryGroups}}{{#unless core}}
                    <a class='btn reveal-group' data-reveal="select-group-{{id}}">{{__ name}}<span class='plus'>+</span></a>
                    {{/unless}}{{/each}}
                  </div>
                </div>
              </div>

            </fieldset>

          </div>
          <div class="yui3-u-1-2">
        
            {{!-- Heritages --}}
            {{#each ancestries}}
            <div class='reveal reveal-heritage' id='reveal-heritage-{{e code}}'>
              {{#each selects}}
                <h3>{{__ name}}</h3>

                <fieldset>
                  {{#each valueGroups}}
                  <div class='select-group{{#if core}} select-group--reveal{{else}} select-group--extra{{/if}}' id='select-group-{{e ../../code}}-{{slug ../name}}-{{id}}'>
                    <h4>{{__ name}}{{#unless core}}<a class='minus unreveal' data-unreveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">&ndash;</a>{{/unless}}</h4>
                    <ul class="wide">
                      {{#each items}}
                        {{#if ../../multiselect}}
                          <li><label for='{{e ../../select}}-{{e code}}'><input type='checkbox' id='{{e ../../select}}-{{e code}}' data-class-option='{{e ../../select}}' name='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{else}}
                          <li><label for='heritage-{{e ../../select}}-{{e code}}'><input type='radio' id='heritage-{{e ../../select}}-{{e code}}' name='{{e ../../select}}' value='{{id}}'> {{__ name}}{{#if badge}}<span class='badge'>{{__ badge}}</span>{{/if}}</label></li>
                        {{/if}}
                      {{/each}}
                    </ul>
                  </div>
                  {{/each}}

                  <div class="select-group-more">
                    {{#unless multiselect}}
                    <ul>
                      <li><label for='heritage-{{e select}}-none'><input type='radio' id='heritage-{{e select}}-none' name='{{e select}}' value='none' checked> {{__ "None"}}</label></li>
                    </ul>
                    {{/unless}}
                    {{#each valueGroups}}{{#unless core}}
                    <a class='btn reveal-group' data-reveal="select-group-{{e ../../code}}-{{slug ../name}}-{{id}}">{{__ name}}<span class="plus">+</span></span></a>
                    {{/unless}}{{/each}}
                  </div>
                </fieldset>
              {{/each}}
            </div>
            {{/each}}
          </div>
        </div>
    
    
        <hr>


        {{!-- Backgrounds --}}
        <h2>{{__ "Background"}}</h2>
        {{!-- <p class='indent'><select id='background'>
          <option value='' selected>{{__ "None"}}</option>
          {{#each backgrounds}}
          <option value='{{id}}'>{{__ name}}</option>
          {{/each}}
        </select></p> --}}

        <fieldset id='backgrounds'>
          {{#each backgroundGroups}}
          <div class='select-group select-group--extra' id='select-group-background-{{id}}'>
            <h4>{{__ name}}<a class='minus unreveal' data-unreveal="select-group-background-{{id}}">&ndash;</a></h4>
            <ul class='wide'>
              {{#each items}}
                <li><label for='background-{{e code}}'><input type='radio' id='background-{{e code}}' name='background' value='{{id}}' data-reveal="{{e code}}"> {{__ name}}</label></li>
              {{/each}}
            </ul>
          </div>
          {{/each}}
          
          <div class='select-group-more'>
            <h4>{{__ "Other"}}</h4>
            <div class="yui3-g">
              <div class="yui3-u-1-3">
                <ul>
                  <li><label for='background-other'><input type='radio' id='background-other' name='background' value='other' checked> {{__ "Other"}}</label></li>
                </ul>
              </div>
              <div class="yui3-u-2-3">
                {{#each backgroundGroups}}
                <a class='btn reveal-group' data-reveal="select-group-background-{{id}}">{{__ name}}<span class='plus'>+</span></a>
